# Let's build an image that is just good enough to build new mkosi images again

[Distribution]
Distribution=fedora
Release=34

[Output]
Format=gpt_ext4
Bootable=yes
Output=fedora.raw

[Packages]
Packages=
        arch-install-scripts
        btrfs-progs
        debootstrap
        dnf
        dosfstools
        git-core
        gnupg
        squashfs-tools
        tar
        veritysetup
